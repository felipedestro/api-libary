    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "typeorm": "^0.3.20",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
